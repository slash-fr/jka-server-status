# IDE-specific files
/.vscode

# Generated files
/var/cache/*
!/var/cache/.gitkeep
/var/log/*
!/var/log/.gitkeep

# Config file
/config.php
# => config.sample.php is versioned

# "About" page content
/templates/_about_page_content.php
# => /templates/_about_page_content.default.php is versioned

# PHPUnit
# phpunit.phar, ...
/phpunit*
# .phpunit.result.cache, ...
/.phpunit*
# If your IDE does not autocomplete PHPUnit classes with just the PHAR file,
# you can put the content of PHPUnit's "src" folder into:
/src/PHPUnit
